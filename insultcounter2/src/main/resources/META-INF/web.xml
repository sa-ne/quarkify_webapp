<web-app>
  <servlet>
    <servlet-name>JSPServlet</servlet-name>
    <servlet-class>com.redhat.servlets.JSPServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSPServlet</servlet-name>
    <url-pattern>*.jsp</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
